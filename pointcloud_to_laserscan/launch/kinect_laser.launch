<launch>
  <!-- kinect and frame ids -->
  <include file="$(find openni_camera)/launch/openni_kinect.launch"/>

  <!-- fake laser -->
  <node pkg="nodelet" type="nodelet" name="kinect_laser" args="standalone pointcloud_to_laserscan/CloudToScan">
    <param name="output_frame_id" value="/openni_depth_frame"/>
    <remap from="cloud" to="/camera/depth/points2"/>
  </node>
</launch>
