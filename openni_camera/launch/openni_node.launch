<launch>
  <node pkg="openni_camera" type="openni_node" name="openni_node1" output="screen" >
    <param name="topic" value="first" />
    <!--param name="device_id" value="2@3" -->
    <param name="device_id" value="#2"/>
    <!--param name="device_id" value="B00367707227042B"/-->
    <rosparam command="load" file="$(find openni_camera)/info/openni_params.yaml" />
  </node>
  <include file="$(find openni_camera)/launch/kinect_frames.launch"/>
</launch>
